pipeline{
agent any

    stages('CI') {
        stage('Checkout') {
            steps {
                echo 'Checkout'
                checkout scm
            }
        }
        stage('Build') {
            steps {
                echo 'Building....!'
                sh 'mvn clean package'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing....Slave node'
                sh 'mvn test'
            }
        }
    }
}
